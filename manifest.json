{
  "name": "Defund Wikipedia",
  "version": "1.1",
  "description": "Deletes donation notices of Wikipedia; which is clearly a biased platform",
  "permissions": ["activeTab", "https://*.wikipedia.org/*"],
  "icons": { "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
        "matches": ["https://*.wikipedia.org/*"],
        "js": ["/content_script.js"],
        "all_frames": true
    }
  ],
  "manifest_version": 2
}